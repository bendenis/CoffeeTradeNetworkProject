make_diffs = function(mydata, var){
vec = vector(mode = "numeric", length = nrow(mydata)^2)
for(i in 1:nrow(mydata)){
diffs = unlist(lapply(mydata[var], function(x) mydata[i,var] - x))
a = 207*(i-1) + 1
b = 207*i
vec[a:b] = diffs
}
return(vec)
}
